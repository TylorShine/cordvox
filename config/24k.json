{
    "preprocess": {
        "length": 24000,
        "pitch_estimation": "fcpe",
        "sample_rate": 24000,
        "n_fft": 1920,
        "frame_size": 480,
        "n_mels": 80
    },
    "trainer": {
        "devices": "auto",
        "max_epochs": 10000,
        "precision": "16-mixed",
        "log_every_n_steps": 50
    },
    "data_module": {
        "cache_dir": "dataset_cache",
        "batch_size": 16,
        "num_workers": 1
    },
    "save": {
        "interval": 400,
        "models_dir": "models"
    },
    "model": {
        "generator": {
            "n_mels": 80,
            "sample_rate": 24000,
            "frame_size": 480,
            "channels": [384, 192, 96, 48, 24],
            "factors": [2, 3, 4, 4, 5]
        },
        "discriminator": {
            "mpd": {
                "periods": [1, 2, 3, 5, 7, 11],
                "channels": 32,
                "channels_mul": 2,
                "max_channels": 256,
                "num_layers": 4
            },
            "mrd": {
                "resolutions": [32, 64, 128, 256],
                "channels": 32,
                "num_layers": 4,
                "max_channels": 256
            }
        }
    }
}